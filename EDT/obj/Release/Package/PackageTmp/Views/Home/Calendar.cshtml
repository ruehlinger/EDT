@using System.Web.Hosting
@using EDT.Models
@{
    ViewBag.Title = "VBA EDT";
    var tilestyle = ViewBag.Dev ? "thumbnail" : "thumbnaildisabled thumbnail disabled";

}

@section styles
{
   
    @Styles.Render("~/Content/fullcalendar.css")
    @Styles.Render("~/Content/fullcalendar.print.css")
}
    <div class="indxLft2">
        <div class="overview">
            <h2>Calendar</h2>
            <br />
            <p>
                Select a date to view upcoming events.
            </p>
        </div>
    </div>
   


<div class="pagecontent1">
    <br />
    <div id='calendar'>
       
    </div>

    </div>

    @section scripts
    {
        <script language="javascript">
            $(document).ready(function() {
                var $lft = $(".indxLft");
                var $rght = $(".indxRight");
                var hl = $lft.height();
                var hr = $rght.height();
                var bp;

                if (hl > hr) {

                    $rght.height(hl);

                } else if (hr > hl) {
                    bp = hr - hl;
                    $lft.height(hr);
                    $lft.css("background-position-y", bp);
                }

            });

        </script>

    }
